package association;

public class Association {

    public static void main(String[] args) {
        
        
        serial_number id = new serial_number(1223412343, "1-1-2020");
        
        
        
        apple first_item = new apple("iphone X", id);
        System.out.println("phone type: "+first_item.item_name + "\n item serial :" + first_item.getID() + "\n   relace_date:    " + first_item.get_relace_date());
    }

}
